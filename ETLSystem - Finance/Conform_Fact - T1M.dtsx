            </DTS:Executable>
            <DTS:Executable
              DTS:refId="Package\Sequence Container - Fact Tables\SD1 - EXPENSE_FACT\EST - Write Job Start info"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="Execute SQL Task"
              DTS:DTSID="{F41CA380-7231-497B-9298-301F3F836F1D}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="EST - Write Job Start info"
              DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2016; © 2015 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{C59EDC99-3C98-4601-BC96-A1AA7DA98EA3}"
                  SQLTask:SqlStatementSource="INSERT INTO [management].ETL_PROCESS_FACT&#xA;SELECT  &#xA;(SELECT DATE_KEY FROM management.DATE_DIM WHERE CONVERT(DATE,FULL_DATE)=CONVERT(DATE,GETDATE()) )&#xA;,(SELECT batch_key FROM management.BATCH_DIM WHERE BATCH_TYPE='batch')&#xA;, T2.TABLE_KEY&#xA;,1&#xA;,GETDATE() ,&#xA;4,&#xA;'Start to update FACT table'&#xA;FROM  management.TABLE_DIM T2&#xA;WHERE T2.[SCHEMA]+'.'+T2.TABLE_NAME='conform_finance.expense_fact'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
              </DTS:ObjectData>
            </DTS:Executable>
            <DTS:Executable
              DTS:refId="Package\Sequence Container - Fact Tables\SD1 - EXPENSE_FACT\Get new records and update columns"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="Execute SQL Task"
              DTS:DTSID="{E1337851-157B-44F4-B6B6-3B3894BB89E8}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="Get new records and update columns"
              DTS:ThreadHint="0">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{C59EDC99-3C98-4601-BC96-A1AA7DA98EA3}"
                  SQLTask:SqlStatementSource="&#xA;/*--------------------------------------------------------------------------------------------------------------------------------------------------------------------&#xA;                            Purpose: ETL for expense_fact&#xA;Developer: Frank Wang&#xA;Version:&#x9;V1.0 20/09/2017 Created by Frank Wang&#xA;&#x9;&#x9;&#x9;V2.0 12/02/2018 Modifed to get all records in extract to conform.&#xA;Note:&#xA;Source table: extract.[expense_transaction]&#xA;Need to map key from conform to conform_finance as they use different primary key.&#xA;---------------------------------------------------------------------------------------------------------------------------------------------------------------------*/&#xA;SET XACT_ABORT , NOCOUNT ON;&#xA;BEGIN TRANSACTION;&#xA;   use dwhb;&#xA;   -- temp table&#xA;    declare @t_expense TABLE&#xA;   (&#xA;&#x9;[Transaction_no] [varchar](10) NULL,&#xA;&#x9;[Transaction_date] [varchar](50) NULL,&#xA;&#x9;[Transaction_date_key] [int] NULL,&#xA;&#x9;[Merchant_Name] [varchar](50) NULL,&#xA;&#x9;[Employee_Email] [varchar](50) NULL,&#xA;&#x9;[Employee_key] [int] NULL,&#xA;&#x9;[Portfolio_key] [int] NULL,&#xA;&#x9;[Service_stream_key] [int] NULL,&#xA;&#x9;[Program_key] [int] NULL,&#xA;&#x9;[Manager_key] [int] NULL,&#xA;&#x9;[Card_Infomation] [varchar](50) NULL,&#xA;&#x9;[Amount] [decimal](10, 2) NULL,&#xA;&#x9;[Net_Amount] [decimal](10, 2) NULL,&#xA;&#x9;[Tax_Amount] [decimal](10, 2) NULL,&#xA;&#x9;[GST] [varchar](10) NULL,&#xA;&#x9;[Description] [varchar](200) NULL,&#xA;&#x9;[Activity code] [varchar](50) NULL,&#xA;&#x9;[Expense_Type] [varchar](50) NULL,&#xA;&#x9;[Line_desription] [varchar](100) NULL,&#xA;&#x9;[Entity] [varchar](10) NULL,&#xA;&#x9;[Costing] [varchar](10) NULL,&#xA;&#x9;[Activity] [varchar](10) NULL,&#xA;&#x9;[Natural] [varchar](10) NULL,&#xA;&#x9;[Staus] [varchar](20) NULL   &#xA;   );&#xA;&#xA;insert into  @t_expense&#xA;--insert into @t_expense, make sure all records are extracted&#xA;select &#xA;  TRY_CONVERT(varchar(10),et.Transaction_no) AS Transaction_no,&#xA;  et.[Transaction_date],&#xA;  dd.date_key as transaction_date_key,&#xA;  et.[Merchant Name],&#xA;  et.[card_holder_email],&#xA;  d.employee_key,&#xA;  f.portfolio_key,&#xA;  f.service_stream_key,&#xA;  f.program_key,&#xA; -- f.position_key,&#xA;  f.manager_key,&#xA;  et.Card_No,&#xA;  TRY_CONVERT([decimal](10, 2),et.amount) AS Amount,&#xA;  TRY_CONVERT([decimal](10, 2),et.Net_Amount) AS Net_Amount,&#xA;  TRY_CONVERT([decimal](10, 2),et.Tax_Amount) AS Tax_Amount,&#xA;  TRY_CONVERT(varchar(10),et.GST) AS GST,&#xA;  left(et.[Description],200),&#xA;  et.[Activity code],&#xA;  et.[Expense Type],&#xA;  et.[Line desription],&#xA;  TRY_CONVERT(varchar(10),et.Entity) AS Entity,&#xA;  TRY_CONVERT(varchar(10),et.Costing) AS Costing,&#xA;  TRY_CONVERT(varchar(10), et.Activity) AS Activity,&#xA;  TRY_CONVERT(varchar(10),et.Natural) AS Natural,&#xA;   'Uncoded' AS [Status]&#xA;  from  &#xA;  ( --Remove all Employee_Costing contain other characters other than 0-9 because it needs to be mathched to id_number which is int&#xA;&#x9;select * from extract.expense_transaction where Employee_Costing not like '%[^0-9]%'&#xA;  ) as et &#xA;  --join on id number and email to make sure the user pickup is right.&#xA;  left join dwhb_new.conform.employee_dim d on et.employee_Costing = d.id_number and d.current_row_indicator = 'Y' &#xA;  --when match the id_number to employee_costing. both two fields:employee_costing and card_holder_email may have wrong records.&#xA;  --and et.Card_Holder_Email = d.ASA_Email&#xA;  left join  dwhb_new.conform.employee_position_fact f on d.employee_key = f.employee_key and f.primary_position_flag = 'P'&#xA;  left join conform_finance.date_dim dd on et.Transaction_Date = CONVERT(VARCHAR(8), dd.full_date, 112)&#xA;&#xA; --update employee_key for those nulls in former step&#xA; update t&#xA; set t.employee_key = d.employee_key&#xA; from @t_expense t left join dwhb_new.conform.employee_dim d on t.Employee_Email = d.ASA_Email --and d.current_row_indicator = 'Y'&#xA; where t.Employee_key is null&#xA;  --update employee_key to unknown if the employee_key is still null&#xA; update t&#xA; set t.employee_key = (select top 1 employee_key from dwhb_new.conform.employee_dim where id_number = 9999999)&#xA; from @t_expense t &#xA; where t.Employee_key is null&#xA; --update keys for those remain employees&#xA;  update t&#xA; set t.portfolio_key=f.portfolio_key,&#xA;  t.service_stream_key=f.service_stream_key,&#xA;  t.Program_key=f.program_key,&#xA; -- f.position_key,&#xA;  t.manager_key=f.manager_key&#xA; from @t_expense t left join&#xA;&#x9;dwhb_new.conform.employee_position_fact f on t.employee_key = f.employee_key and f.primary_position_flag ='P'&#xA; where   t.portfolio_key is null;&#xA;&#xA;&#xA;  --update keys for those lsot employees&#xA;  update t&#xA; set t.portfolio_key = (select top 1 portfolio_key from dwhb_new.conform.portfolio_dim where portfolio_code = 99999),&#xA; t.service_stream_key = (select top 1 service_stream_key from dwhb_new.conform.service_stream_dim where service_stream_code = 99999),&#xA; t.Program_key = (select top 1 Program_key from dwhb_new.conform.Program_dim where Program_code = 99999)&#xA; from @t_expense t &#xA; where t.portfolio_key is null;&#xA;&#xA; --update manager key&#xA;  update t&#xA; set t.manager_key = (select top 1 employee_key from dwhb_new.conform.employee_dim where id_number = 9999999)&#xA; from @t_expense t &#xA; where t.manager_key is null&#xA;&#xA; --update status &#xA; -- If costing information is NULL then Uncoded else Waiting for approval&#xA; update  @t_expense&#xA; set Staus = 'Waiting for approval'&#xA; where len(ltrim(rtrim(costing)))&gt;0;&#xA;&#xA; --update keys:&#xA;--&#x9;[Program_key],&#xA;&#xA;WITH  t_program_map (fp_key,p_key)&#xA;AS  &#xA;(select fp.program_key as fp_key, p.program_key as p_key &#xA;from conform_finance.program_dim fp inner join dwhb_new.conform.program_dim p on fp.program_code = p.program_code&#xA;and fp.current_row_indicator = 'y' and p.current_row_indicator = 'y')&#xA;UPDATE t&#xA;SET t.program_key = t_program_map.fp_key&#xA;FROM @t_expense AS t inner join t_program_map&#xA;on t.program_key = t_program_map.p_key;&#xA;&#xA; --update keys:&#xA;--[Portfolio_key],&#xA;WITH  t_portfolio_map (fp_key,p_key)&#xA;AS  &#xA;(select fp.portfolio_key as fp_key, p.portfolio_key as p_key &#xA;from conform_finance.portfolio_dim fp inner join dwhb_new.conform.portfolio_dim p on fp.portfolio_code = p.portfolio_code&#xA;and fp.current_row_indicator = 'y' and p.current_row_indicator = 'y')&#xA;UPDATE t&#xA;SET t.portfolio_key = t_portfolio_map.fp_key&#xA;FROM @t_expense AS t inner join t_portfolio_map&#xA;on t.portfolio_key = t_portfolio_map.p_key;&#xA;&#xA; --update keys:&#xA;--[Service_stream_key],&#xA;WITH  t_Service_stream_map (fp_key,p_key)&#xA;AS  &#xA;(select fp.Service_stream_key as fp_key, p.Service_stream_key as p_key &#xA;from conform_finance.Service_stream_dim fp inner join dwhb_new.conform.Service_stream_dim p on fp.Service_stream_code = p.Service_stream_code&#xA;and fp.current_row_indicator = 'y' and p.current_row_indicator = 'y')&#xA;UPDATE t&#xA;SET t.Service_stream_key = t_Service_stream_map.fp_key&#xA;FROM @t_expense AS t inner join t_Service_stream_map&#xA;on t.Service_stream_key = t_Service_stream_map.p_key;&#xA;&#xA; --update keys:&#xA;--[employee_key],&#xA;WITH  t_employee_map (fp_key,p_key)&#xA;AS  &#xA;(   &#xA;&#x9;select max(fp.employee_key) as fp_key, (p.employee_key) as p_key &#xA;&#x9;from conform_finance.employee_dim fp inner join dwhb_new.conform.employee_dim p on fp.id_number = p.id_number&#xA;&#x9;group by p.employee_key&#xA;)&#xA;UPDATE t&#xA;SET t.employee_key = t_employee_map.fp_key&#xA;FROM @t_expense AS t inner join t_employee_map&#xA;on t.employee_key = t_employee_map.p_key;&#xA;&#xA; --update keys:&#xA;--[manager_key],&#xA;WITH  t_employee_map (fp_key,p_key)&#xA;AS  &#xA;(&#xA;&#x9;select max(fp.employee_key) as fp_key, (p.employee_key) as p_key &#xA;&#x9;from conform_finance.employee_dim fp inner join dwhb_new.conform.employee_dim p on fp.id_number = p.id_number&#xA;&#x9;group by p.employee_key&#xA;)&#xA;UPDATE t&#xA;SET t.manager_key = t_employee_map.fp_key&#xA;FROM @t_expense AS t inner join t_employee_map&#xA;on t.manager_key = t_employee_map.p_key;&#xA;&#xA;--Insert result to destination&#xA; insert into conform_finance.expense_fact&#xA; (&#xA;[Transaction_no],&#xA;&#x9;[Transaction_date_key],&#xA;&#x9;[Merchant_Name] ,&#xA;&#x9;[Employee_Email],&#xA;&#x9;[Employee_key],&#xA;&#x9;[Portfolio_key],&#xA;&#x9;[Service_stream_key],&#xA;&#x9;[Program_key],&#xA;&#x9;[Manager_key],&#xA;&#x9;[Card_Infomation] ,&#xA;&#x9;[Amount] ,&#xA;&#x9;[Net_Amount] ,&#xA;&#x9;[Tax_Amount] ,&#xA;&#x9;[GST] ,&#xA;&#x9;[Description],&#xA;&#x9;[Activity code] ,&#xA;&#x9;[Expense_Type] ,&#xA;&#x9;[Line_desription] ,&#xA;&#x9;[Entity],&#xA;&#x9;[Costing] ,&#xA;&#x9;[Activity] ,&#xA;&#x9;[Natural] ,&#xA;&#x9;[Staus] &#xA;     )&#xA;select &#xA;[Transaction_no],&#xA;&#x9;[Transaction_date_key],&#xA;&#x9;[Merchant_Name] ,&#xA;&#x9;[Employee_Email],&#xA;&#x9;[Employee_key],&#xA;&#x9;[Portfolio_key],&#xA;&#x9;[Service_stream_key],&#xA;&#x9;[Program_key],&#xA;&#x9;[Manager_key],&#xA;&#x9;[Card_Infomation] ,&#xA;&#x9;[Amount] ,&#xA;&#x9;[Net_Amount] ,&#xA;&#x9;[Tax_Amount] ,&#xA;&#x9;[GST] ,&#xA;&#x9;[Description],&#xA;&#x9;[Activity code] ,&#xA;&#x9;[Expense_Type] ,&#xA;&#x9;[Line_desription] ,&#xA;&#x9;[Entity],&#xA;&#x9;[Costing] ,&#xA;&#x9;[Activity] ,&#xA;&#x9;[Natural] ,&#xA;&#x9;[Staus] &#xA; from @t_expense;&#xA;&#xA; COMMIT TRANSACTION;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
              </DTS:ObjectData>
            </DTS:Executable>
