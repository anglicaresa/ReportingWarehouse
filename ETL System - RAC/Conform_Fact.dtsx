    <DTS:Executable
      DTS:refId="Package\pay_period_summary_dollars_role_type_report"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{32B6ECD2-60C3-4A50-9E51-1CD9D04511D6}"
