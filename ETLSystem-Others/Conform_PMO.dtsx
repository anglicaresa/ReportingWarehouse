          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{F32AC200-FDAF-406D-B1AA-81D3B9956A8D}"
              SQLTask:SqlStatementSource="--insert original date&#xA;insert into [conform_PMO].[project_health](&#xA;&#x9;&#x9;[Portfolio]&#xA;      ,[Project_Name]&#xA;&#x9;  ,[Overall_Health]&#xA;      ,[Project_Sponsor]&#xA;      ,[Project_Manager]&#xA;      ,[Start_Date]&#xA;      ,[Complete_Date]&#xA;&#x9;  ,[Project_Type]&#xA;      ,[Project_Phases]&#xA;      ,[Project_Phases_Init]&#xA;      ,[Project_Phases_Plan]&#xA;      ,[Project_Phases_Implem]&#xA;      ,[Project_Phases_Review]&#xA;      ,[Project_Phases_Hold]&#xA;      ,[Project_Health_Budget]&#xA;      ,[Project_Health_Time]&#xA;      ,[Project_Health_Risk]&#xA;&#x9;  ,[Destination]&#xA;&#x9;  ,[Budget_Expenditure_to_Date]&#xA;&#x9;  ,[Budget_Amount]&#xA;&#x9;  ,Project_Category&#xA;&#x9;  )&#xA;&#xA;&#x9;   SELECT s.[Portfolio],s.[Project Title],s.[Overall Health], s.[Project Sponsor],s.[Project manager],s.[Scheduled Start Date],s.[Scheduled Completion Date],s.[Project Type],s.[Project Stage]&#xA;&#x9;   ,0,0,0,0,0,0,0,0,s.[Destination - Project health x phase]&#xA;&#x9;   ,s.[Budget - Expenditure to Date]&#xA;&#x9;   ,s.[Budget Amount]&#xA;&#x9;   ,s.[Project Category]&#xA;&#x9;   FROM [extract_PMO].owssvrExport s;&#xA;&#xA;--update Project Phases&#xA;update t&#xA;set &#xA;t.[Project_Phases_Init] = (case when SUBSTRING([Project_Phases],1,1) = '1' then 1 else 0 end),&#xA;t.[Project_Phases_Plan] = (case when SUBSTRING([Project_Phases],1,1) = '2' then 1 else 0 end),&#xA;t.[Project_Phases_Implem] = (case when SUBSTRING([Project_Phases],1,1) = '3' then 1 else 0 end),&#xA;t.[Project_Phases_Review] = (case when SUBSTRING([Project_Phases],1,1) = '4' then 1 else 0 end),&#xA;t.[Project_Phases_Hold] = (case when SUBSTRING([Project_Phases],1,1) = '5' then 1 else 0 end)&#xA;&#xA;from  [conform_PMO].[project_health] t;&#xA;&#xA;--update Project Health, three columns&#xA;update t&#xA;set &#xA;t.[Project_Health_Budget] = (case when o2.[Project on Budget] = 'On Track - No Issues' then 1 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   when o2.[Project on Budget] = 'Cautionary Issues' then 2 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    when o2.[Project on Budget] = 'Definite Issues' then 3 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when o2.[Project on Budget] = 'Not started' then 4 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else 0 end),&#xA;t.[Project_Health_Time] = (case when o2.[Project on Time] = 'On Track - No Issues' then 1 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   when o2.[Project on Time] = 'Cautionary Issues' then 2 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    when o2.[Project on Time] = 'Definite Issues' then 3 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when o2.[Project on Time] = 'Not started' then 4 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else 0 end),&#xA;t.[Project_Health_Risk] =(case when o2.[Project Risk] = 'On Track - No Issues' then 1 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   when o2.[Project Risk] = 'Cautionary Issues' then 2 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    when o2.[Project Risk] = 'Definite Issues' then 3 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;when o2.[Project Risk] = 'Not started' then 4 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else 0 end)&#xA;&#xA;&#x9;   FROM [conform_PMO].[project_health] as  t inner join  [extract_PMO].owssvrExport o2 on t.[Project_Name] = o2.[Project Title]&#xA;&#xA;&#xA;--update complete percentage by JS at 07.01.2020&#xA;update [conform_PMO].[project_health]&#xA;set Completed = convert(decimal(10,4),DATEDIFF(day,start_date, GETDATE())) / convert(decimal(10,4),DATEDIFF(day,start_date, Complete_Date))&#xA;where Project_Phases in ('1. Investigate','2. Plan','3. Implement')&#xA;&#xA;--If the project is not completed and has gone past it’s project complete date = leave the % complete column ‘blank’. &#xA;update  [conform_PMO].[project_health]&#xA;set  Completed =NULL&#xA;where Project_Phases in ('1. Investigate','2. Plan','3. Implement')&#xA;and Complete_Date&lt; GETDATE()&#xA;&#xA;&#xA;--If the project start_date is furture, then leave the % complete column ‘blank’. &#xA;update  [conform_PMO].[project_health]&#xA;set  Completed =NULL&#xA;where Project_Phases in ('1. Investigate','2. Plan','3. Implement')&#xA;and Start_Date &gt; GETDATE()&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
