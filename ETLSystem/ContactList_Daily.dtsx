Peoples_People.Sex /* Sex lookup */  LEFT JOIN HomeTeam_LookUpValues AS PeopleTitle ON PeopleTitle.Company = 
Peoples_People.Company AND PeopleTitle.Lookup = 10 AND PeopleTitle.ID = Peoples_People.Title /* Title lookup */ LEFT JOIN 
HomeTeam_LookUpValues AS PeopleSpecialNeeds ON PeopleSpecialNeeds.Company = Peoples_People.Company AND PeopleSpecialNeeds.Lookup 
= 12 AND PeopleSpecialNeeds.ID = Peoples_People.SpecialNeedsGroup /* Special Needs lookup */ LEFT JOIN HomeTeam_LookUpValues AS PeopleEthnicOrigin ON PeopleEthnicOrigin.Company = Peoples_People.Company AND PeopleEthnicOrigin.Lookup = 13 AND PeopleEthnicOrigin.ID = Peoples_People.EthnicOrigin /* Ethnic Origin lookup */ LEFT JOIN HomeTeam_LookUpValues AS PeopleReligion ON PeopleReligion.Company = Peoples_People.Company AND PeopleReligion.Lookup = 346 AND PeopleReligion.ID = Peoples_People.ReligionOrBelief /* Religion/Belief Lookup */ LEFT JOIN HomeTeam_LookUpValues AS PeopleType ON PeopleType.Company = Peoples_People.Company AND PeopleType.Lookup = 14 AND PeopleType.ID = Peoples_People.Type /* Type lookup */  LEFT JOIN HomeTeam_LookUpValues AS PeopleMaritalStatus ON PeopleMaritalStatus.Company = Peoples_People.Company AND PeopleMaritalStatus.Lookup = 15 AND PeopleMaritalStatus.ID = Peoples_People.MaritalStatus /* Marital Status lookup */ LEFT JOIN Peoples_Employer ON Peoples_Employer.Company = Peoples_People.Company AND Peoples_Employer.ID = Peoples_People.Employer LEFT JOIN HomeTeam_LookUpValues AS DisabilityLookup ON DisabilityLookup.Company = Peoples_People.Company AND DisabilityLookup.Lookup = 564 AND DisabilityLookup.ID = Peoples_People.Disability LEFT JOIN HomeTeam_LookUpValues AS NonEnglish ON NonEnglish.Company = Peoples_People.Company AND NonEnglish.Lookup = 561 AND NonEnglish.ID = Peoples_People.NonEnglishSpeakingBackground inner join dbo.Companies_Company on Peoples_People.company = dbo.Companies_Company.id		LEFT JOIN Peoples_People_TotalWeeklyIncome ON Peoples_People_TotalWeeklyIncome.Company = Peoples_People.Company AND Peoples_People_TotalWeeklyIncome.ID = Peoples_People.ID LEFT JOIN Peoples_People_TotalWeeklyWagesIncome ON Peoples_People_TotalWeeklyWagesIncome.Company = Peoples_People.Company AND Peoples_People_TotalWeeklyWagesIncome.ID = Peoples_People.ID WHERE (Peoples_People.Company = 1)  AND (1=0  OR Peoples_People.CurrentTenant = 1 )  
ORDER BY Peoples_People.Surname ASC</property>
