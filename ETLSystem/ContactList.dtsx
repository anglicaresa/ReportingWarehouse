                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select DISTINCT 
sd.Client_ID,
p.Preferred_Name,p.Given_Names,p.Last_Name,p.Salutation,
landline.Communication_Point_Number as landline,
mobile.Communication_Point_Number as mobile,
email.Communication_Point_Number as email,
--sd.Service_Type_Code,
--c2.Business_Unit_Code,
--o2.Organisation_Name,
fp.Description as program
from Service_Delivery sd
left join Person p on sd.Client_ID = p.Person_ID 
left join Client c on sd.Client_ID = c.Client_ID 
left join Person_Comm_Point_Type as landline on p.Person_ID = landline.Person_ID and landline.Communication_Point_Type_Code = 1
left join Person_Comm_Point_Type as mobile on p.Person_ID = mobile.Person_ID and mobile.Communication_Point_Type_Code = 3
left join Person_Comm_Point_Type as email on p.Person_ID = email.Person_ID and email.Communication_Point_Type_Code = 5
left join Funding_Program fp on sd.Funding_Prog_Code = fp.Funding_Prog_Code 
--left join Service_Provision sp on sd.Service_Type_Code = sp.Service_Type_Code 
--left join Centre c2 on c.Centre_ID = c2.Centre_ID 
--left join Organisation o2 on c.Centre_ID =sd.Organisation_ID 
where (sd.To_Date &gt; getdate() or sd.To_Date is null)</property>
