    <DTS:Executable
      DTS:refId="Package\Update fact table for Facebook"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{712D61A5-ADC1-4398-8255-C38FF276F1AF}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update fact table for Facebook"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{094D9D19-2D64-4DE9-868D-9C4B61727B5C}"
          SQLTask:SqlStatementSource="/* -------------------------------------------------------------------------------&#xA;      Purpose: export data for ASA Facebook Workplace from Data Warehouse&#xA;&#x9;  Developer: Jerry Shen&#xA;&#x9;  Notes:&#xA;&#x9;   1. First Created by JS at 17/07/2019&#xA;&#x9;   2. Add codes for comparison&#xA;&#xA;&#xA;--------------------------------------------------------------------------------*/&#xA;&#xA;&#xA;&#xA;--back yesterday data&#xA;truncate table conform.facebook_fact_yesterday;&#xA;insert into conform.facebook_fact_yesterday&#xA;select * from conform.facebook_fact;&#xA;&#xA;&#xA;--populate today's data for fact table&#xA;&#xA;truncate table conform.facebook_fact&#xA;&#xA;insert into conform.facebook_fact&#xA;select &#xA;[Anglicare Email Address]&#xA;,[Preferred Name]&#xA;,[Family Name],&#xA;[Job Title]&#xA;,portfolio_name&#xA;,service_stream_name&#xA;,[Organisation Unit Description]&#xA;,NULL&#xA;,[Work Location]&#xA;,'en_GB'&#xA;,NULL -- Manager_Email: will update later&#xA;,'SSO'&#xA;,[Date Hired]&#xA;,program_name&#xA;,[Id Number]&#xA;,NULL -- Ad_UserID: will update later&#xA;from conform.e3learning_fact&#xA;where Active='Y'&#xA;&#xA;--update Manager_Email&#xA;&#xA;update t1&#xA;set t1.[Manager Email] = t2.[Anglicare Email Address]&#xA;&#xA;from conform.facebook_fact as t1&#xA;&#xA;inner join &#xA;&#xA;(&#xA;select &#xA;emp_manager.[Id Number],&#xA;emp_manager.[Supervisor ID],&#xA;emp_email.[Anglicare Email Address]&#xA;from &#xA;(select distinct [Id Number],[Supervisor ID]&#xA;from &#xA;conform.e3learning_fact) as emp_manager&#xA;inner join &#xA;(&#xA;select distinct [Id Number],[Anglicare Email Address]&#xA;from conform.e3learning_fact&#xA;) as emp_email&#xA;on emp_manager.[Supervisor ID]=emp_email.[Id Number]&#xA;&#xA;) as t2&#xA;&#xA;on t1.[Employee ID]= t2.[Id Number]&#xA;&#xA;--update Ad_userID&#xA;&#xA;update t1&#xA;set t1.[AD_UserID] = t2.AD_UserID&#xA;&#xA;from conform.facebook_fact as t1&#xA;&#xA;inner join &#xA;(&#xA;select id_number, AD_UserID &#xA;from dwhb_new.conform.employee_dim &#xA;where current_row_indicator='Y'&#xA;) as t2&#xA;on t1.[Employee ID] = t2.id_number&#xA;&#xA;--update conform.facebook_fact_datacheck&#xA;truncate table conform.facebook_fact_datacheck;&#xA;&#xA;&#xA;--new employees&#xA;insert into conform.facebook_fact_datacheck&#xA;select *,'New Employee'&#xA;from conform.facebook_fact&#xA;where [Employee ID] not in &#xA;(select [Employee ID]  from conform.facebook_fact_yesterday);&#xA;&#xA;--terminate employee&#xA;insert into conform.facebook_fact_datacheck&#xA;select *,'Terminated Employee'&#xA;from conform.facebook_fact_yesterday&#xA;where [Employee ID] not in &#xA;(select [Employee ID]  from conform.facebook_fact);&#xA;&#xA;--email change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Email Changed: old - '+isnull(b.Email,'NULL') + ' new: '+ isnull(a.Email,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and a.Email &lt;&gt; b.Email&#xA;and (b.Email is not NULL or len(b.Email)&lt;&gt;0)&#xA;and (a.Email is not NULL or len(a.Email)&lt;&gt;0);&#xA;&#xA;--first name change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Preferred Name Changed: old - '+isnull(b.[First Name],'NULL') + ' new: '+ isnull(a.[First Name],'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.[First Name]&lt;&gt;b.[First Name] );&#xA;&#xA;--last name change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Last Name Changed: old - '+isnull(b.[Last Name],'NULL') + ' new: '+ isnull(a.[Last Name],'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.[Last Name]&lt;&gt;b.[Last Name] );&#xA;&#xA;&#xA;&#xA;--job title change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Job Title Changed: old - '+isnull(b.[Job Title],'NULL') + ' new: '+ isnull(a.[Job Title],'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.[Job Title] &lt;&gt; b.[Job Title]);&#xA;&#xA;--manager email change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Manager Email Changed: old - '+isnull(b.[Manager Email],'NULL') + ' new: '+ isnull(a.[Manager Email],'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.[Manager Email]&lt;&gt; b.[Manager Email]);&#xA;&#xA;--ad userid change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'AD UserID Changed: old - '+isnull(b.AD_UserID,'NULL') + ' new: '+ isnull(a.AD_UserID,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.AD_UserID &lt;&gt; b.AD_UserID);&#xA;&#xA;&#xA;--org change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Department Changed: old - '+isnull(b.Department,'NULL') + ' new: '+ isnull(a.Department,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Department&lt;&gt;b.Department );&#xA;&#xA;&#xA;-- Division&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Division Changed: old - '+isnull(b.Division,'NULL') + ' new: '+ isnull(a.Division,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Division&lt;&gt;b.Division )&#xA;and a.[Employee ID] not in (select [Employee ID] from conform.facebook_fact_datacheck);&#xA;&#xA;update t1&#xA;set t1.Check_Results = t1.Check_Results + '|'+ t2.checkresults&#xA;from conform.facebook_fact_datacheck t1&#xA;inner join&#xA;(&#xA;select a.*,'Division Changed: old - '+isnull(b.Division,'NULL') + ' new: '+ isnull(a.Division,'NULL') as checkresults&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Division&lt;&gt;b.Division )&#xA;and a.[Employee ID]  in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;) as t2&#xA;on t1.[Employee ID]= t2.[Employee ID]&#xA;&#xA;--Organization&#xA;&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Organization Changed: old - '+isnull(b.Organization,'NULL') + ' new: '+ isnull(a.Organization,'NULL') &#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Organization&lt;&gt;b.Organization )&#xA;and a.[Employee ID] not in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;&#xA;update t1&#xA;set t1.Check_Results = t1.Check_Results + '|'+ t2.checkresults&#xA;from conform.facebook_fact_datacheck t1&#xA;inner join&#xA;(&#xA;select a.*,'Organization Changed: old - '+isnull(b.Organization,'NULL') + ' new: '+ isnull(a.Organization,'NULL') as checkresults&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Organization&lt;&gt;b.Organization )&#xA;and a.[Employee ID] in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;) as t2&#xA;on t1.[Employee ID]= t2.[Employee ID]&#xA;&#xA;--Location&#xA;&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Location Changed: old - '+isnull(b.Location,'NULL') + ' new: '+ isnull(a.Location,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Location&lt;&gt;b.Location )&#xA;and a.[Employee ID] not in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;&#xA;&#xA;update t1&#xA;set t1.Check_Results = t1.Check_Results + '|'+ t2.checkresults&#xA;from conform.facebook_fact_datacheck t1&#xA;inner join&#xA;(&#xA;select a.*,'Location Changed: old - '+isnull(b.Location,'NULL') + ' new: '+ isnull(a.Location,'NULL')  as checkresults&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Location&lt;&gt;b.Location )&#xA;and a.[Employee ID]  in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;) as t2&#xA;on t1.[Employee ID]= t2.[Employee ID]&#xA;&#xA;--team change&#xA;insert into conform.facebook_fact_datacheck&#xA;select a.*,'Team Changed: old - '+isnull(b.Team,'NULL') + ' new: '+ isnull(a.Team,'NULL')&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Team &lt;&gt; b.Team)&#xA;and a.[Employee ID] not in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;&#xA;&#xA;update t1&#xA;set t1.Check_Results = t1.Check_Results + '|'+ t2.checkresults&#xA;from conform.facebook_fact_datacheck t1&#xA;inner join&#xA;(select a.*,'Team Changed: old - '+isnull(b.Team,'NULL') + ' new: '+ isnull(a.Team,'NULL') as checkresults&#xA;from conform.facebook_fact as a&#xA;inner join conform.facebook_fact_yesterday as b&#xA;on a.[Employee ID]=b.[Employee ID]&#xA;and (a.Team &lt;&gt; b.Team)&#xA;and a.[Employee ID] not in (select [Employee ID] from conform.facebook_fact_datacheck)&#xA;) as t2&#xA;on t1.[Employee ID]= t2.[Employee ID]" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
