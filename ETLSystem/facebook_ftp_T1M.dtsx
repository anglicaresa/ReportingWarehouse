      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{094D9D19-2D64-4DE9-868D-9C4B61727B5C}"
          SQLTask:SqlStatementSource="/* -------------------------------------------------------------------------------&#xA;      Purpose: export data for ASA Facebook Workplace from Data Warehouse&#xA;&#x9;  Developer: Jerry Shen&#xA;&#x9;  Notes:&#xA;&#x9;   1. First Created by JS at 17/07/2019&#xA;&#xA;&#xA;&#xA;--------------------------------------------------------------------------------*/&#xA;&#xA;truncate table conform.facebook_fact&#xA;&#xA;insert into conform.facebook_fact&#xA;select &#xA;[Anglicare Email Address]&#xA;,[Preferred Name]&#xA;,[Family Name],&#xA;[Job Title]&#xA;,portfolio_name&#xA;,service_stream_name&#xA;,[Organisation Unit Description]&#xA;,NULL&#xA;,[Work Location]&#xA;,'en_GB'&#xA;,NULL -- Manager_Email: will update later&#xA;,'SSO'&#xA;,[Date Hired]&#xA;,program_name&#xA;,[Id Number]&#xA;,NULL -- Ad_UserID: will update later&#xA;from conform.e3learning_fact;&#xA;&#xA;--update Manager_Email&#xA;&#xA;update t1&#xA;set t1.[Manager Email] = t2.[Anglicare Email Address]&#xA;&#xA;from conform.facebook_fact as t1&#xA;&#xA;inner join &#xA;&#xA;(&#xA;select &#xA;emp_manager.[Id Number],&#xA;emp_manager.[Supervisor ID],&#xA;emp_email.[Anglicare Email Address]&#xA;from &#xA;(select distinct [Id Number],[Supervisor ID]&#xA;from &#xA;conform.e3learning_fact) as emp_manager&#xA;inner join &#xA;(&#xA;select distinct [Id Number],[Anglicare Email Address]&#xA;from conform.e3learning_fact&#xA;) as emp_email&#xA;on emp_manager.[Supervisor ID]=emp_email.[Id Number]&#xA;&#xA;) as t2&#xA;&#xA;on t1.[Employee ID]= t2.[Id Number]&#xA;&#xA;--update Ad_userID&#xA;&#xA;update t1&#xA;set t1.[AD_UserID] = t2.AD_UserID&#xA;&#xA;from conform.facebook_fact as t1&#xA;&#xA;inner join &#xA;(&#xA;select id_number, AD_UserID &#xA;from dwhb_new.conform.employee_dim &#xA;where current_row_indicator='Y'&#xA;) as t2&#xA;on t1.[Employee ID] = t2.id_number" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
