        <SQLTask:SqlTaskData
          SQLTask:Connection="{E4FB9867-2768-4626-BEEB-E246D28A7731}"
          SQLTask:SqlStatementSource="/*--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------&#xA;                                 Purpose: extract information from AD and import into DWH table&#xA;                                 Developer: Jerry Shen&#xA;                                 Version: &#xA;                                           Created at 17/07/2017 by JS&#xA;                                 Notes: &#xA;                                         1. we need to create linked server to connect AD then can use linked server and SQL to query AD information as below                     &#xA;&#xA;---------Step1: create linked server at Master database to connect AD&#xA;&#xA;USE [master]&#xA;GO&#xA;EXEC master.dbo.sp_addlinkedserver @server = N'ADSI', @srvproduct=N'Active Directory Service Interfaces', @provider=N'ADSDSOObject', @datasrc=N'adsdatasource'&#xA;EXEC master.dbo.sp_addlinkedsrvlogin @rmtsrvname=N'ADSI',@useself=N'False',@locallogin=NULL,@rmtuser=N'anglicare-sa\asaappdev',@rmtpassword='frank1106'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'collation compatible',  @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'data access', @optvalue=N'true'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'dist', @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'pub', @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'rpc', @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'rpc out', @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'sub', @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'connect timeout', @optvalue=N'0'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'collation name', @optvalue=null&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'lazy schema validation',  @optvalue=N'false'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'query timeout', @optvalue=N'0'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'use remote collation',  @optvalue=N'true'&#xA;GO&#xA;EXEC master.dbo.sp_serveroption @server=N'ADSI', @optname=N'remote proc transaction promotion', @optvalue=N'true'&#xA;GO&#xA;&#xA;--Step2: create view to get all AD information&#xA;CREATE VIEW [AdView] AS &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''A*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''B*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''C*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''D*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''E*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''F*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''G*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''H*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''I*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''J*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''K*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''L*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''M*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''N*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''O*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''P*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''Q*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''R*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''S*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''T*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''U*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''V*''') UNION ALL &#xA;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''W*''') UNION ALL &#xA;&#x9;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''X*''') UNION ALL &#xA;&#x9;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''Y*''') UNION ALL &#xA;&#x9;SELECT * FROM OPENQUERY(ADSI, '&#xA;    SELECT  sn, pager, displayName, telephoneNumber, &#xA;mail, mobile, facsimileTelephoneNumber, department, physicalDeliveryOfficeName, givenname ,userPrincipalName,&#x9;sAMAccountName,employeeID&#xA;    FROM ''LDAP://anglicare-sa.org.au''&#xA;    WHERE objectClass = ''User'' AND objectCategory = ''Person'' AND samAccountName = ''Z*''') &#xA;&#xA;-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------*/&#xA;--truncate table &#xA;truncate table extract.adusers;&#xA;&#xA;&#xA;&#xA;--update new data&#xA;insert into extract.adusers&#xA;select &#x9;[sAMAccountName] ,&#xA;&#x9;[userPrincipalName] ,&#xA;&#x9;[givenname],&#xA;&#x9;[physicalDeliveryOfficeName],&#xA;&#x9;[department],&#xA;&#x9;[facsimileTelephoneNumber],&#xA;&#x9;[mobile],&#xA;&#x9;[mail],&#xA;&#x9;[telephoneNumber] ,&#xA;&#x9;[displayName] ,&#xA;&#x9;[pager] ,&#xA;&#x9;[sn] ,&#xA;&#x9;[employeeID]&#xA; from [master].dbo.[AdView];&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Update employee_dim table"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{38AAA76E-14CD-4EC4-86E4-535F092E57C9}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update employee_dim table"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{E4FB9867-2768-4626-BEEB-E246D28A7731}"
          SQLTask:SqlStatementSource="--clear all&#xA;update conform.employee_dim&#xA;set user_id=NULL, AD_UserID=NULL, ASA_Email=NULL&#xA;where current_row_indicator='Y'&#xA;&#xA;&#xA;--add T1 userid&#xA;update t1&#xA;set t1.user_id =t2.user_id &#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.hrhmn_empl_ref as t2&#xA;on t1.id_number=t2.id_number &#xA;and t1.current_row_indicator='Y'&#xA;&#xA;--update ad_userid and asa_email using userid&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on t1.user_id = t2. sAMAccountName &#xA;and t1.current_row_indicator='Y'&#xA;&#xA;&#xA;--update ad_userid and asa_email which userid cannot match&#xA;--using given_name and family_name&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on t1.given_name=t2.givenname&#xA;and  (replace(t1.family_name,' ','')=replace(t2.sn,' ','')&#x9;&#x9;or &#x9; replace(t1.family_name,'-','')=replace(t2.sn,' ','') or &#x9; replace(t1.family_name,'''','')=replace(t2.sn,' ','') )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;--using preferred_name and family_name&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on t1.preferred_name=t2.givenname&#xA;and  (replace(t1.family_name,' ','')=replace(t2.sn,' ','')&#x9;&#x9;or &#x9; replace(t1.family_name,'-','')=replace(t2.sn,' ','') or &#x9; replace(t1.family_name,'''','')=replace(t2.sn,' ','') )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;&#xA;--using email&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  (t1.street_email1=t2.mail&#x9; or t1.postal_email1=t2.mail   )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;-- using mobile&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  ( replace(t1.street_telephone3,' ','')=replace(t2.mobile,' ','')&#x9; or replace(t1.street_mobile1,' ','')=replace(t2.mobile,' ',''))&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;&#xA;-- using famaly_name and left(given_name,5)&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.family_name=t2.sn &#xA;and  (left(t1.given_name,5)=left(t2.givenname,5) or  left(t1.preferred_name,5)=left(t2.givenname,5)    )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.family_name=t2.sn &#xA;and  (left(t1.given_name,4)=left(t2.givenname,4) or  left(t1.preferred_name,4)=left(t2.givenname,4)    )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.family_name=t2.sn &#xA;and  (left(t1.given_name,3)=left(t2.givenname,3) or  left(t1.preferred_name,3)=left(t2.givenname,3)    )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.family_name=t2.sn &#xA;and  (left(t1.given_name,2)=left(t2.givenname,2) or  left(t1.preferred_name,2)=left(t2.givenname,2)    )&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;&#xA;--for 25 employee we have to manual map&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='5454'&#xA;and t2.sAMAccountName='nsinghsandhu'&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='5478'&#xA;and t2.sAMAccountName='Ania.Mroz'&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='5734'&#xA;and t2.sAMAccountName='Jithesh.Varghese'&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA;&#xA;update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='5915'&#xA;and t2.sAMAccountName='Jacqualine.Poidevin'&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA; update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='5938'&#xA;and t2.sAMAccountName='Bikash.Pokharel'&#xA;and t1.current_row_indicator='Y'&#xA;and t1.ad_userid is null&#xA;&#xA; update t1&#xA;set t1.ad_userid = t2.sAMAccountName, &#xA;    t1.ASA_Email=t2.mail&#xA;from conform.employee_dim &#x9;  as t1&#xA;inner join &#x9;extract.ADUsers as t2&#xA;on  t1.id_number ='6136'&#xA;and t2.sAMAccountName='La