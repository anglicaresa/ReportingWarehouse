    <DTS:Executable
      DTS:refId="Package\Update employee_survery_fact_new"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{A0C1815A-A6D8-44B3-9573-7AE24A1ED943}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update employee_survery_fact_new"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CB2DEBB3-DFB3-4D21-AA37-114454AECEDD}"
          SQLTask:SqlStatementSource="use dwhb;&#xA;&#xA;/*-------------------------------------------------------------------------------&#xA;This is written at 20.02.2020 basing on Chris De Cicco's requirement&#xA;Source file was exported from T1 ETL: EMPLOYEE_SURVEY_BASIC_DATASOURCE&#xA;&#xA;--------------------------------------------------------------------------------*/&#xA;&#xA;&#xA;--import source table&#xA;&#xA;--CREATE TABLE extract.employee_survey_t1data(&#xA;--    [Position_Code] varchar(50),&#xA;--    [Description] varchar(50),&#xA;--    [Position_Code_Status] varchar(50),&#xA;--    [Employee_Type_Job_Defaults] varchar(50),&#xA;--    [Employee_Class_Job_Defaults] varchar(50),&#xA;--    [Organisation_Unit_Job_Defaults] varchar(50),&#xA;--    [Work_Location_Job_Defaults] varchar(50),&#xA;--    [Id_Number] varchar(50),&#xA;--    [Family_Name] varchar(50),&#xA;--    [Given_Name] varchar(50),&#xA;--    [Preferred_Name] varchar(50),&#xA;--    [Date_of_Birth] varchar(50),&#xA;--    [Effective_Date_Job] varchar(50),&#xA;--    [Position_Code1] varchar(50),&#xA;--    [Supervisor_Id] varchar(50),&#xA;--    [Supervisor_Position] varchar(50),&#xA;--    [Position_Type] varchar(50),&#xA;--    [Unit_Name] varchar(50)&#xA;--)&#xA;&#xA;&#xA;&#xA;--select distinct Id_Number  from  extract.employee_survey_t1data where Id_Number&lt;&gt;'0' and Position_Type in ('A','P')&#xA;&#xA;--select *  from  extract.employee_survey_t1data where Id_Number&lt;&gt;'0'&#xA;&#xA;--select Position_Type, count(*)  from  extract.employee_survey_t1data where Id_Number&lt;&gt;'0' group by Position_Type&#xA;&#xA;--Position_Type&#x9;(No column name)&#xA;--A&#x9;34&#xA;--P&#x9;1791&#xA;--S&#x9;239&#xA;&#xA;--------------------------------------------------------------------------------------------------------------------------&#xA;&#xA;&#xA;--drop table  [conform].[employee_survery_fact_new]&#xA;&#xA;--CREATE TABLE [conform].[employee_survery_fact_new](&#xA;--&#x9;[Employee/payroll number] [varchar](100) NULL,&#xA;--&#x9;[AD username] [varchar](100) NULL,&#xA;--&#x9;[First name] [varchar](100) NULL,&#xA;--&#x9;[Last name] [varchar](100) NULL,&#xA;--     [Preferred name] [varchar](100) NULL,&#xA;--&#x9;[Email address] [varchar](100) NULL,&#xA;--&#x9;[Manager_EmpId] [varchar](100) NULL,&#xA;--&#x9;[Manager name (Reports to – Supervisor Name)] [varchar](100) NULL,&#xA;--&#x9;[Position] [varchar](100) NULL,&#xA;--&#x9;[Position Code] [varchar](100) NULL,&#xA;--&#x9;[Work Location (Job Defaults)] [varchar](100) NULL,&#xA;--    [Organisation Unit (Job Defaults)] [varchar](100) NULL,&#xA;--&#x9;[Supervisor Position] [varchar](100) NULL,&#xA;--&#x9;[DOB]  [varchar](100) NULL,&#xA;--&#x9;[Age] [decimal](10, 2) NULL,&#xA;--&#x9;[Age_Group] [varchar](100) NULL,&#xA;--&#x9;[Hire_Date] [datetime] NULL,&#xA;--&#x9;[Servie_Years] [decimal](10, 2) NULL,&#xA;--&#x9;[Length of Service] [varchar](100) NULL,&#xA;--&#x9;[Gender] [varchar](100) NULL,&#xA;--&#x9;[Employment Status] [varchar](100) NULL,&#xA;--&#x9;[CALD] [varchar](100) NULL,&#xA;--&#x9;[ATSI] [varchar](100) NULL,&#xA;--&#x9;[Disability] [varchar](100) NULL,&#xA;--&#x9;[Position Type]  [varchar](100) NULL&#xA;--) ON [PRIMARY]&#xA;--GO&#xA;&#xA;--alter table [conform].[employee_survery_fact_new] add [Supervisor Position Name] [varchar](100) &#xA;--alter table dwhf.hris.employee_survery_fact_new add [Supervisor Position Name] [varchar](100) &#xA;&#xA;--------------------------------------------------------------------------------------------------------------------------&#xA;&#xA;&#xA;truncate table conform.employee_survery_fact_new;&#xA;&#xA;--basing on techone positon export file&#xA;insert into conform.employee_survery_fact_new&#xA;select &#xA; [Id_Number],&#xA; NULL,&#xA; [Given_Name],&#xA; [Family_Name],&#xA;  [Preferred_Name],&#xA;  NULL,&#xA;  NULL,&#xA;  NULL,&#xA;  [Description],&#xA;  [Position_Code],&#xA;  [Work_Location_Job_Defaults],&#xA;  [Organisation_Unit_Job_Defaults]+' - '+[Unit_Name],&#xA;  [Supervisor_Position],&#xA;  [Date_of_Birth],&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  null,&#xA;  [Position_Type],&#xA;  null&#xA;&#xA;from extract.employee_survey_t1data&#xA;where Id_Number&lt;&gt;'0'&#xA;and Position_Type&lt;&gt;'S'&#xA;&#xA;&#xA;--- ----------employee has Acting position still need to get their primary position--------&#xA;--- added by JS at 24.02.2020&#xA;&#xA;insert into conform.employee_survery_fact_new&#xA;select d1.id_number&#x9; as [Id Number],&#xA;       d1.ad_userid,&#xA;&#x9;   d1.given_name,&#xA;&#x9;   d1.family_name,&#xA;&#x9;   d1.preferred_name,&#xA;&#x9;   d1.ASA_Email,&#xA;&#x9;   d4.id_number,&#xA;&#x9;   NULL,&#xA;&#x9;   d2.position_name,&#xA;&#x9;   d2.position_code,&#xA;&#x9;   d2.work_location,&#xA;&#x9;   NULL ,-- orgunit &#xA;&#x9;   null,&#xA;&#xA;&#x9;   convert(varchar(10),d1.date_of_birth,103),&#xA;&#x9;   NULL,&#xA;&#x9;   NULL,&#xA;&#x9;   d8.full_date,&#xA;&#x9;   NULL,&#xA;&#x9;   NULL,&#xA;&#x9;   d1.gender,&#xA;&#x9;   d1.employee_type,&#xA;&#x9;   case d1.ethnic_group&#xA;&#x9;   when 'CALD' then 'Yes'&#xA;&#x9;   else 'No'&#xA;&#x9;   end,&#xA;&#xA;&#x9;    case d1.ethnic_group&#xA;&#x9;   when 'ATSI' then 'Yes'&#xA;&#x9;   else 'No'&#xA;&#x9;   end,&#xA;&#x9;  Case  d1.empl_disability&#xA;&#x9;  when 'Y' then 'Yes'&#xA;&#x9;  else 'No'&#xA;&#x9;  end,&#xA;&#x9;  'A-P',&#xA;&#x9;  null&#xA;&#x9;   &#xA; from dwhb_new.conform.employee_position_fact   as f1&#xA;inner join dwhb_new.conform.employee_dim as d1 on f1.employee_key=d1.employee_key and d1.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.position_dim as d2 on f1.position_key=d2.position_key and d2.current_row_indicator='Y'&#xA;inner join &#x9; dwhb_new.conform.position_dim as d3 on f1.manager_position_key=d3.position_key and d3.current_row_indicator='Y'&#xA;left join dwhb_new.conform.employee_dim as d4 on f1.manager_key =d4.employee_key and d4.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.employee_temp as f2 on f1.employee_key =f2.employee_key and f2.primary_position_flag='P'&#xA;inner join dwhb_new.conform.program_dim as d5 on f1.program_key=d5.program_key and d5.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.service_stream_dim as d6 on f1.service_stream_key=d6.service_stream_key and d6.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.portfolio_dim as d7 on f1.portfolio_key=d7.portfolio_key and d7.current_row_indicator='Y' &#xA;inner join dwhb_new.conform.date_dim as d8 on f1.position_start_date_key = d8.date_key&#xA;inner join (select &#xA;           distinct &#xA;           id_number &#xA;           from extract.employee_survey_t1data&#xA;           where Position_Type='A'&#xA;&#x9;&#x9;   ) as f3 on d1.id_number= f3.id_number&#xA;where f1.primary_position_flag='P'&#xA;&#xA;&#xA;&#xA;update t1&#xA;set&#xA;&#xA;t1.[Organisation Unit (Job Defaults)]= t2.unitname,&#xA;t1.[Work Location (Job Defaults)] = t2.Work_Location_Job_Defaults,&#xA;t1.[Supervisor Position]= t2.Supervisor_Position&#xA;&#xA;from  conform.employee_survery_fact_new as t1&#xA;inner join &#xA;(&#xA;select &#xA; [Id_Number],&#xA; [Given_Name],&#xA; [Family_Name],&#xA;  [Preferred_Name],&#xA;  &#xA;  [Description],&#xA;  [Position_Code],&#xA;  [Work_Location_Job_Defaults],&#xA;  [Organisation_Unit_Job_Defaults]+' - '+[Unit_Name] as unitname,&#xA;  [Supervisor_Position],&#xA;  [Date_of_Birth],&#xA;  &#xA;  [Position_Type]&#xA;&#xA;from extract.employee_survey_t1data&#xA;where  Position_Code in &#xA;(&#xA;select distinct d1.position_code&#xA;from dwhb_new.conform.EMPLOYEE_POSITION_FACT as f1&#xA;inner join dwhb_new.conform.POSITION_DIM as d1 on f1.position_key= d1.position_key and f1.primary_position_flag='P'&#xA;and f1.employee_key in (select employee_key from dwhb_new.conform.EMPLOYEE_POSITION_FACT where primary_position_flag='A')&#xA;)&#xA;) as t2 &#xA;on t1.[Position Code]= t2.Position_Code&#xA;and t1.[Position Type]='A-P'&#xA;&#xA;&#xA;------------------------------------------update manager infomation--------------------------------------------------------------&#xA;--update some supervisor positon missnig data basing on data warehouse positon_fact table&#xA;&#xA;update f1&#xA;set f1.[Supervisor Position] = f2.manager_position_code&#xA;from conform.employee_survery_fact_new as f1&#xA;inner join &#xA;(select &#xA;d1.id_number,&#xA;d3.position_code as emp_position_code,&#xA;d2.position_code as manager_position_code&#xA;&#xA;from dwhb_new.conform.EMPLOYEE_POSITION_FACT as t1&#xA;inner join dwhb_new.conform.employee_Dim as d1 on t1.employee_key= d1.employee_key and d1.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.POSITION_DIM as d2 on t1.manager_position_key= d2.position_key and d2.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.POSITION_DIM as d3 on t1.position_key= d3.position_key and d3.current_row_indicator='Y'&#xA;) as f2&#xA;on f1.[Employee/payroll number] = f2.id_number&#xA;and f1.[Position Code]= f2.emp_position_code&#xA;and f1. [Supervisor Position] = '' &#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;update t1&#xA;set t1.[Organisation Unit (Job Defaults)] = t2.[Organisation Unit (Job Defaults)]&#xA;from conform.employee_survery_fact_new as t1&#xA;inner join &#xA;(&#xA;&#xA;select  distinct [Supervisor Position], [Organisation Unit (Job Defaults)]&#xA;from conform.employee_survery_fact_new&#xA;where  [Organisation Unit (Job Defaults)] &lt;&gt; ' - ' &#xA;) as t2&#xA;on t1.[Organisation Unit (Job Defaults)] = ' - ' &#xA;and t1.[Supervisor Position] = t2.[Supervisor Position]&#xA;&#xA;&#xA;&#xA;--update basing on employee_dim&#xA;update t1&#xA;set t1.CALD = case d1.ethnic_group&#xA;&#x9;   when 'CALD' then 'Yes'&#xA;&#x9;   else 'No'&#xA;&#x9;   end,&#xA;&#xA;&#x9;    t1.ATSI = case d1.ethnic_group&#xA;&#x9;   when 'ATSI' then 'Yes'&#xA;&#x9;   else 'No'&#xA;&#x9;   end,&#xA;&#xA;&#x9;   t1.Disability =  Case  d1.empl_disability&#xA;&#x9;  when 'Y' then 'Yes'&#xA;&#x9;  else 'No'&#xA;&#x9;  end,&#xA;&#xA;&#x9;  t1.Gender= d1.gender,&#xA;&#x9;  t1.[Employment Status]= d1.employee_type,&#xA;&#xA;&#x9;  t1.[AD username]= d1.ad_userid,&#xA;&#xA;&#x9;  t1.[Email address] =d1.ASA_Email&#xA;from conform.employee_survery_fact_new as t1&#xA;inner join dwhb_new.conform.employee_dim as d1 &#xA;on t1.[Employee/payroll number]= d1.id_number&#xA;and d1.current_row_indicator='Y'&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;--update manager info&#xA;update t1 &#xA;set t1.Manager_EmpId = t2.emp_id,&#xA;    t1.[Manager name (Reports to – Supervisor Name)] = t2.fullname&#xA;from conform.employee_survery_fact_new as t1&#xA;inner join &#xA;   (select &#xA;    [Employee/payroll number] as emp_id,&#xA;&#x9;[First name]+' '+[Last Name] as fullname,&#xA;&#x9;[Position Code] as position_code&#xA;  &#xA; from conform.employee_survery_fact_new&#xA; where [Position Code]&lt;&gt;'9999999'&#xA; ) as t2 &#xA; on t1.[Supervisor Position] = t2.position_code&#xA; and t1.[Supervisor Position]&lt;&gt;'9999999'&#xA; &#xA;&#xA;&#xA; --- some employee's report to manager's secondary position, not only acting positon&#xA; update t1&#xA; set t1.Manager_EmpId= t2.Id_Number,&#xA;     t1.[Manager name (Reports to – Supervisor Name)] = t2.Given_Name+' '+t2.Family_Name&#xA; from conform.employee_survery_fact_new as t1&#xA; inner join &#xA; (&#xA;&#xA; select * from   extract.employee_survey_t1data where Position_Type='S'&#xA;&#xA; ) as t2&#xA; on t1.[Supervisor Position] = t2.Position_Code&#xA; and t1.Manager_EmpId is NULL&#xA;&#xA;&#xA; ----update manager position name&#xA; update t1&#xA; set t1.[Supervisor Position Name] = d1.position_name&#xA; from conform.employee_survery_fact_new as t1&#xA; inner join dwhb_new.conform.position_dim as d1  on t1.[Supervisor Position]= d1.position_code and d1.current_row_indicator='Y'&#xA;&#xA;&#xA;-- update age&#xA;update conform.employee_survery_fact_new&#xA;set Age= DATEDIFF(DAY,convert(date,[DOB],103) ,getdate())/365.00;&#xA;&#xA;--update conform.employee_survery_fact_new&#xA;--set Age_Group = case &#xA;--when age &lt;18 then 'Under 18'&#xA;--when age&gt;=18 and age &lt;25 then '18-24'&#xA;--when age&gt;=25 and age &lt;35 then '25-34'&#xA;--when age&gt;=35 and age &lt;45 then '35-44'&#xA;--when age&gt;=45 and age &lt;55 then '45-54'&#xA;--when age&gt;=55 and age &lt;65 then '55-64'&#xA;--when age&gt;=65  then 'Over 65'&#xA;--end;&#xA;&#xA;&#xA;update conform.employee_survery_fact_new&#xA;set Age_Group = case &#xA;when age &lt;=20 then 'Under 20'&#xA;when age&gt;20 and age &lt;=30 then '20-30'&#xA;when age&gt;30 and age &lt;=40 then '31-40'&#xA;when age&gt;40 and age &lt;=50 then '41-50'&#xA;when age&gt;50 and age &lt;=60 then '51-60'&#xA;when age&gt;60 and age &lt;=70 then '61-70'&#xA;when age&gt;70 and age &lt;=80  then '71-80'&#xA;else 'Over 80'&#xA;end;&#xA;&#xA;&#xA;-- service years&#xA;&#xA;update t1&#xA;set t1.Hire_Date= t2.full_date&#xA;from conform.employee_survery_fact_new as t1&#xA;inner join &#xA;(select &#xA;d1.id_number,&#xA;d2.full_date&#xA;from dwhb_new.conform.EMPLOYEE_PROFILE_FACT as f1&#xA;inner join dwhb_new.conform.employee_dim as d1 on f1.employee_key= d1.employee_key and d1.current_row_indicator='Y'&#xA;inner join dwhb_new.conform.DATE_DIM as d2 on f1.recruitment_date_key= d2.date_key&#xA;) as t2&#xA;on t1.[Employee/payroll number]= t2.id_number&#xA;&#xA;&#xA;update conform.employee_survery_fact_new&#xA;set [Servie_Years]= DATEDIFF(DAY,Hire_Date, getdate()) / 365.00;&#xA;&#xA;update conform.employee_survery_fact_new&#xA;set [Length of Service] = case &#xA;when [Servie_Years] &lt;=0.5 then '0 - 6 months'&#xA;when [Servie_Years]&gt;0.5 and [Servie_Years] &lt;=1 then '6.1month - 1 year'&#xA;when [Servie_Years]&gt;1 and [Servie_Years] &lt;=2 then '1.1 - 2 years'&#xA;when [Servie_Years]&gt;2 and [Servie_Years] &lt;=4 then '2.1 – 4 years'&#xA;when [Servie_Years]&gt;4 and [Servie_Years] &lt;=6 then '4.1 - 6 years'&#xA;when [Servie_Years]&gt;6 and [Servie_Years] &lt;=10 then '6.1 – 10 years'&#xA;when  [Servie_Years]&gt;10 then 'Over 10 years'&#xA;end;&#xA;&#xA;&#xA;&#xA;&#xA;-- --delete Causual employees who havent got payment in the last 35 days&#xA;&#xA;delete from conform.employee_survery_fact_new&#xA;where [Employee/payroll number] in (&#xA;&#xA; &#xA;select&#xA;t1.id_number&#xA;&#xA;from dwhb_new.conform.employee_dim as t1&#xA;inner join&#xA;(&#xA;select employee_key, max(pay_period_date_key) as last_pay_period_date_key from dwhb_new.conform.employee_payment_summary_fact&#xA;group by employee_key&#xA;) as t2 on t1.employee_key = t2.employee_key and t1.current_row_indicator='Y' and t1.employee_type='Casual'&#xA;inner join dwhb_new.conform.date_dim as t3 on t2.last_pay_period_date_key= t3.date_key and DATEDIFF(day,t3.full_date,getdate())&gt;35&#xA;&#xA;)&#xA;&#xA;&#xA;--update work location with description&#xA;update t1&#xA;set  t1.[Work Location (Job Defaults)] = d1.work_location&#xA;from  conform.employee_survery_fact_new as t1&#xA;inner join dwhb_new.conform.POSITION_DIM as d1 on t1.[position code] = d1.position_code and d1.current_row_indicator='Y'&#xA;and d1.work_location like (t1.[Work Location (Job Defaults)]+'%')&#xA;&#xA;&#xA;--update position types&#xA;update  conform.employee_survery_fact_new&#xA;set [Position Type] = case [Position Type] when 'A' then 'Acting' when 'A-P' then 'Acting-Primary' else 'Primary' end &#xA;&#xA;&#xA;--delivery to front table &#xA;truncate table dwhf.hris.employee_survery_fact_new;&#xA;insert into dwhf.hris.employee_survery_fact_new&#xA;select * from conform.employee_survery_fact_new" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
