      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{74000DC6-2227-4537-9EF2-3A6FD561006D}"
          SQLTask:SqlStatementSource="&#xA; /* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------&#xA;                                                   &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;     Purpose: get employee data for e3learning&#xA;                                                     Creator: Jerry Shen&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; Version:&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;          V1.0 created by JS&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;     Note: haven't found way for Cost Centre - Location Description ,Cost Centre -Service Type Code,Cost Centre -Service Type Description  &#xA;&#xA;&#xA;--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */&#xA;use dwhb &#xA;--get main data from dwh tables.&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#xA;&#xA;truncate table conform.e3learning_fact;&#xA;&#xA;--get active employee&#xA;insert into conform.e3learning_fact&#xA;select d1.id_number&#x9; as [Id Number],&#xA;       d1.preferred_name as  [Preferred Name],&#xA;&#x9;   d1.family_name as [Family Name],&#xA;&#x9;   d2.position_name as  [Job Title],&#xA;&#x9;   d1.date_of_birth as [Date of Birth],&#xA;&#x9;   '-' as [Work Group],&#xA;&#x9;   '-' as [Work Location],&#xA;&#x9;   d2.position_code as [Employee Position Code],&#xA;&#x9;&#x9;d3.position_code as [Supervisor Position Code],&#xA;&#x9;&#x9; d4.id_number as [Supervisor ID] ,&#xA;&#x9;&#x9; d4.preferred_name+' '+d4.family_name as [Supervisor Name],&#xA;&#x9;&#x9;'----' as [Cost Centre - Location Code],&#xA;&#x9;&#x9;'-' as&#x9;[Cost Centre - Location Description]  ,&#xA;&#x9;&#x9;'-' as &#x9;[Cost Centre -Service Type Code],&#xA;&#x9;&#x9;   '-' as&#x9;[Cost Centre -Service Type Description]&#x9; ,&#xA;&#x9;&#x9;   '-----' as [Organisation Unit Code],&#xA;&#x9;&#x9;   '                                                                                      ' as&#x9;[Organisation Unit Description],&#xA;&#x9;&#x9;  f2.hired_date as  [Date Hired],&#xA;&#x9;&#x9;  d1.street_email1 as&#x9;[Anglicare Email Address],&#xA;&#x9;&#x9;  '' as&#x9;[Termination Date],&#xA;&#x9;&#x9;  'Y' as&#x9;[Active],&#xA;&#x9;&#x9;  d5.program_name as [program_name],&#xA;&#x9;&#x9;  d5.program_code,&#xA;&#x9;&#x9;  d6.service_stream_name,&#xA;&#x9;&#x9;  d6.service_stream_code, &#xA;&#x9;&#x9;  d7.portfolio_name,&#xA;&#x9;&#x9;  d7.portfolio_code&#xA;&#xA; from conform.employee_position_fact   as f1&#xA;inner join conform.employee_dim as d1 on f1.employee_key=d1.employee_key and d1.current_row_indicator='Y'&#xA;inner join conform.position_dim as d2 on f1.position_key=d2.position_key and d2.current_row_indicator='Y'&#xA;inner join &#x9; conform.position_dim as d3 on f1.manager_position_key=d3.position_key and d3.current_row_indicator='Y'&#xA;left join conform.employee_dim as d4 on f1.manager_key =d4.employee_key and d4.current_row_indicator='Y'&#xA;inner join conform.employee_temp as f2 on f1.employee_key =f2.employee_key and f2.primary_position_flag='P'&#xA;inner join conform.program_dim as d5 on f1.program_key=d5.program_key and d5.current_row_indicator='Y'&#xA;inner join conform.service_stream_dim as d6 on f1.service_stream_key=d6.service_stream_key and d6.current_row_indicator='Y'&#xA;inner join conform.portfolio_dim as d7 on f1.portfolio_key=d7.portfolio_key and d7.current_row_indicator='Y' &#xA;where f1.primary_position_flag='P'&#xA;&#xA;&#xA;--get terminate employee in the past 14 days.&#xA;insert into conform.e3learning_fact&#xA;select d1.id_number&#x9; as [Id Number],&#xA;       d1.preferred_name as  [Preferred Name],&#xA;&#x9;   d1.family_name as [Family Name],&#xA;&#x9;   d2.position_name as  [Job Title],&#xA;&#x9;   d1.date_of_birth as [Date of Birth],&#xA;&#x9;   '-' as [Work Group],&#xA;&#x9;   '-' as [Work Location],&#xA;&#x9;   d2.position_code as [Employee Position Code],&#xA;&#xA;&#x9;&#x9;'-' as [Supervisor Position Code],&#xA;&#xA;&#x9;&#x9;'-' as [Supervisor ID] ,&#xA;&#x9;&#x9;'-'  as [Supervisor Name],&#xA;&#xA;&#x9;&#x9;'----' as [Cost Centre - Location Code],&#xA;&#x9;&#x9;'-' as&#x9;[Cost Centre - Location Description]  ,&#xA;&#x9;&#x9;'-' as &#x9;[Cost Centre -Service Type Code],&#xA;&#x9;&#x9;   '-' as&#x9;[Cost Centre -Service Type Description]&#x9; ,&#xA;&#x9;&#x9;   '-----' as [Organisation Unit Code],&#xA;&#x9;&#x9;   '                                                                                      ' as&#x9;[Organisation Unit Description],&#xA;&#x9;&#x9;  f2.hired_date as  [Date Hired],&#xA;&#x9;&#x9;  d1.street_email1 as&#x9;[Anglicare Email Address],&#xA;&#x9;&#x9;  convert(date,d8.full_date) as&#x9;[Termination Date],&#xA;&#x9;&#x9;  'N' as&#x9;[Active],&#xA;&#x9;&#x9;  d5.program_name as [program_name],&#xA;&#x9;&#x9;  d5.program_code,&#xA;&#x9;&#x9;  d6.service_stream_name,&#xA;&#x9;&#x9;  d6.service_stream_code, &#xA;&#x9;&#x9;  d7.portfolio_name,&#xA;&#x9;&#x9;  d7.portfolio_code&#xA;&#xA; from conform.employee_termination_fact   as f1&#xA;inner join conform.employee_dim as d1 on f1.employee_key=d1.employee_key and d1.current_row_indicator='N'&#xA;inner join conform.position_dim as d2 on f1.position_key=d2.position_key and d2.current_row_indicator='Y'&#xA;--inner join &#x9; conform.position_dim as d3 on f1.manager_position_key=d3.position_key and d3.current_row_indicator='Y'&#xA;--left join conform.employee_dim as d4 on f1.manager_key =d4.employee_key and d4.current_row_indicator='Y'&#xA;inner join conform.employee_temp as f2 on f1.employee_key =f2.employee_key and f2.primary_position_flag='P'&#xA;inner join conform.program_dim as d5 on f1.program_key=d5.program_key and d5.current_row_indicator='Y'&#xA;inner join conform.service_stream_dim as d6 on f1.service_stream_key=d6.service_stream_key and d6.current_row_indicator='Y'&#xA;inner join conform.portfolio_dim as d7 on f1.portfolio_key=d7.portfolio_key and d7.current_row_indicator='Y' &#xA;inner join conform.date_dim as d8 on f1.termination_date_key=d8.date_key and d8.full_date&gt;=getdate()-14&#xA;&#xA;&#xA;--update work_location&#xA;update t1&#xA;set t1.[Work Location]=i3.WORK_LOCATION2  ,t1.[Work Group]=i3.WORK_GROUP   ,&#xA;t1.[Organisation Unit Code]=i3.DEPARTMENT_CODE&#xA;from  conform.e3learning_fact as t1&#xA;inner join &#xA;(  select &#xA;g1.*,&#xA;g2.code_descr as work_location2&#x9;&#x9;  &#xA;&#xA;from &#xA;(&#xA;&#xA;select t1.id_number,t1.position_code,t1.work_location  ,t1.WORK_GROUP  ,t1.DEPARTMENT_CODE&#xA;from extract.hrhmn_empl_job_pos as t1&#xA;inner join&#xA; (select id_number, position_code,max(EFFECTIVE_DATE) as EFFECTIVE_DATE&#xA;  from &#x9;extract.hrhmn_empl_job_pos where POSITION_TYPE_IND='P'&#x9;and id_number not in (select id_number from extract.hrhmn_emplterm_dtl)&#x9;&#x9;  and end_date ='1900-01-01 00:00:00.000'&#xA;  group by id_number, position_code   )&#xA;as t2&#xA;on t1.id_number =t2.id_number &#xA;and t1.POSITION_CODE=t2.POSITION_CODE&#xA;and t1.EFFECTIVE_DATE=t2.EFFECTIVE_DATE&#x9;&#xA;where t1.POSITION_TYPE_IND='P'&#xA;and t1.id_number not in (select id_number from extract.hrhmn_emplterm_dtl)&#x9;&#x9;&#x9;&#x9;and t1.end_date ='1900-01-01 00:00:00.000'&#xA;) as g1 &#xA;inner join &#xA;(SELECT code_descr,code_id  as location&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;  FROM [DWHB].[extract].[hrstc_code]   where code_type like '%Location%'&#x9;&#x9;) as g2&#xA;  on g1.WORK_LOCATION=g2.location&#xA;  ) as i3&#xA;  on t1.[Id Number]=i3.ID_NUMBER&#xA;  and t1.[Employee Position Code]=i3.POSITION_CODE&#xA;&#xA;--repalce comma in [Work Location]&#xA;&#xA;update conform.e3learning_fact&#xA;set [Work Location]=REPLACE([Work Location],',',' ');&#xA;&#xA;--update asaemail basing on ID&#xA;&#xA;  update t1&#xA;  set &#x9;t1.[Anglicare Email Address]=d1.asa_email&#xA;  from &#x9;conform.e3learning_fact as t1&#xA;  inner join   conform.employee_dim  as d1 &#xA;  on t1.[Id Number] =d1.id_number and d1.current_row_indicator='Y'&#xA;  and d1.asa_email is not null&#xA;&#xA;&#xA; --costing code&#xA; --haven't got way to update costing description&#xA;&#xA;  update &#x9; g1&#xA;  set g1.[Cost Centre - Location Code]=g2.seln_code1&#xA;  from   &#x9;conform.e3learning_fact as g1&#xA;  inner join &#xA;  (&#xA;&#xA;select t1.* from extract.hrsys_usrseln_dtl    as t1&#xA;inner join &#xA;(&#xA;select  entity_key_1, max(last_mod_datei) as  last_mod_datei&#xA;from extract.hrsys_usrseln_dtl &#xA;where &#x9; entity_type ='EPS'&#xA;group by  entity_key_1) as t2&#xA;on t1.entity_key_1 = t2.entity_key_1&#xA;and t1.last_mod_datei=t2.last_mod_datei&#xA;where t1.entity_type ='EPS' &#xA;) as g2&#xA;on g1.[Id Number] =g2.entity_key_1&#xA;&#xA;--update work group with details&#xA;update g1&#xA;set   g1.[Work Group]=g2.code_descr&#xA;from &#x9;&#x9;conform.e3learning_fact as g1&#xA;inner join &#xA;(  SELECT t1.*&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;  FROM [DWHB].[extract].[hrstc_code]   &#x9;as t1&#xA;  inner join &#xA;  (select code_id,max(last_mod_datei) as  last_mod_datei&#xA;  from &#x9;[DWHB].[extract].[hrstc_code] &#xA;  where code_type='WORK_GROUP'&#xA;  group by &#x9;code_id&#x9;   ) as t2&#xA;  on t1.code_id=t2.code_id &#xA;  and t1.last_mod_datei=t2.last_mod_datei&#x9;  &#xA;  where t1.code_type like '%WORK_GROUP%'&#xA;) as g2&#xA;on &#x9; g1.[Work Group]=g2.code_id &#xA;&#xA;update conform.e3learning_fact&#xA;set [Work Group]='-' where len(ltrim([Work Group]))=0&#xA;&#xA;--update org unit description&#xA;&#xA;update g1&#xA;set&#x9;  g1.[Organisation Unit Description]=g2.program_name&#xA;from   conform.e3learning_fact as g1&#xA;inner join&#xA;( select DISTINCT t1.HIERARCHY_CODE as program_code&#xA;  , t1.hierarchy_descr as program_name&#xA;  from extract.[HRSYS_HIERARCH_CTL] t1,&#xA;  extract.HRSYS_HIERARCH_ITM t2,&#xA;  extract.HRSYS_HIERARCH_LVL t3&#xA;  where t1.HIERARCHY_CODE = t2.HIERARCHY_CODE COLLATE DATABASE_DEFAULT&#xA;  and t2.CURRENT_LEVEL = t3.HIERARCHY_LEVEL&#xA;  and t1.HIERARCHY_TYPE = 'ORGUNIT'&#xA;  and t2.PARENT_NODE != ''&#xA;  and t3.LEVEL_DESC  IN ('Level 4','Level 5')&#xA;) as g2&#xA;on g1.[Organisation Unit Code] = g2.program_code&#xA;&#xA;&#xA;--if no program use service_stream_name&#xA;update &#x9;   conform.e3learning_fact&#x9;&#xA;set [Organisation Unit Description]=service_stream_name&#xA;    where  len(ltrim([Organisation Unit Description] ))=0&#xA;&#xA;&#xA;--if no program and service_stream_name, use portfolio name&#x9;&#xA;update &#x9;   conform.e3learning_fact&#x9;&#xA;set [Organisation Unit Description]=portfolio_name&#xA;    where  service_stream_code='99999' and program_code='99999'&#xA;&#xA;--update some information for terminate employee&#xA;update conform.e3learning_fact&#xA;set [organisation Unit code]=[program_code]&#xA;where active ='N'&#xA;&#xA;&#xA;--repalce comma in [Work Location],[Job Title]&#xA;&#xA;update conform.e3learning_fact&#xA;set [Work Location]=REPLACE([Work Location],',',' '),[Job Title]=REPLACE([Job Title],',',' '),&#xA;   [program_name]=REPLACE([program_name],',',' '),[service_stream_name]=REPLACE([service_stream_name],',',' '),[portfolio_name]=replace([portfolio_name],',',' '),&#xA;   [Organisation Unit Description] =replace([Organisation Unit Description] ,',', ' ');&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
