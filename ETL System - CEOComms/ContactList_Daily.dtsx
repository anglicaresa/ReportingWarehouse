-- IsNull((select Value from Peoples_People_UserDefinedFields_Values ppudf where ppudf.Company = 
--Peoples_People.Company  and ppudf.ObjectReference = Peoples_People.ID and ppudf.UserDefinedField = 77),
--'') as [Date Signed],
-- 
--IsNull((select Value from Peoples_People_UserDefinedFields_Values ppudf where ppudf.Company = Peoples_People.Company  and 
--ppudf.ObjectReference = Peoples_People.ID and ppudf.UserDefinedField = 432),
--'') as [F01A Orientation Awareness ACP2 Updated],
-- 
--IsNull((select Value from Peoples_People_UserDefinedFields_Values ppudf where ppudf.Company = Peoples_People.Company  and 
--ppudf.ObjectReference = Peoples_People.ID and ppudf.UserDefinedField = 433),
--'') as [F01 Orientation Awareness ACP2 Uploaded] 

FROM 
Occupancies_Occupancy INNER JOIN
                         Occupancies_People ON Occupancies_Occupancy.Company = Occupancies_People.Company AND Occupancies_Occupancy.id = Occupancies_People.Occupancy AND
                         --Occupancies_Occupancy.CurrentOccupancy = 1
						 --AND
						 Occupancies_Occupancy.Type &lt;&gt; 99 AND Occupancies_Occupancy.Company = 1 inner JOIN
                             (SELECT        Occupancy, MIN(People) AS people
                               FROM            Occupancies_People AS Occupancies_People_1
                               --WHERE        (Status = '1')
                               GROUP BY Occupancy) AS OCM ON Occupancies_Occupancy.id = OCM.Occupancy
							   --AND Occupancies_Occupancy.CurrentOccupancy = 1
							   AND Occupancies_Occupancy.Type &lt;&gt; 99 AND
                         Occupancies_Occupancy.Company = 1
						 INNER JOIN
                         Occupancies_People AS occupancies_people2 ON Occupancies_Occupancy.Company = occupancies_people2.Company AND Occupancies_Occupancy.id = occupancies_people2.Occupancy --AND
                         --Occupancies_People.Status = '1'
INNER JOIN Peoples_People on Peoples_People.ID = OCM.people
LEFT JOIN dbo.Peoples_Address   AS PeopleAddress ON PeopleAddress.Company = Peoples_People.Company AND 
PeopleAddress.Person = Peoples_People.ID AND PeopleAddress.[Default] = 1 /* Address Details if exists */  

LEFT JOIN HomeTeam_LookUpValues AS PeopleSex ON PeopleSex.Company = Peoples_People.Company AND PeopleSex.Lookup = 11 AND PeopleSex.ID = Peoples_People.Sex /* Sex lookup */  

LEFT JOIN HomeTeam_LookUpValues AS PeopleTitle ON PeopleTitle.Company = 
Peoples_People.Company AND PeopleTitle.Lookup = 10 AND PeopleTitle.ID = Peoples_People.Title /* Title lookup */ 

LEFT JOIN 
HomeTeam_LookUpValues AS PeopleSpecialNeeds ON PeopleSpecialNeeds.Company = Peoples_People.Company AND PeopleSpecialNeeds.Lookup 
= 12 AND PeopleSpecialNeeds.ID = Peoples_People.SpecialNeedsGroup /* Special Needs lookup */ 

LEFT JOIN HomeTeam_LookUpValues AS PeopleEthnicOrigin ON PeopleEthnicOrigin.Company = Peoples_People.Company AND PeopleEthnicOrigin.Lookup = 13 AND PeopleEthnicOrigin.ID = Peoples_People.EthnicOrigin /* Ethnic Origin lookup */ 
LEFT JOIN HomeTeam_LookUpValues AS PeopleReligion ON PeopleReligion.Company = Peoples_People.Company AND PeopleReligion.Lookup = 346 AND PeopleReligion.ID = Peoples_People.ReligionOrBelief /* Religion/Belief Lookup */ 
LEFT JOIN HomeTeam_LookUpValues AS PeopleType ON PeopleType.Company = Peoples_People.Company AND PeopleType.Lookup = 14 AND PeopleType.ID = Peoples_People.Type /* Type lookup */  
LEFT JOIN HomeTeam_LookUpValues AS PeopleMaritalStatus ON PeopleMaritalStatus.Company = Peoples_People.Company AND PeopleMaritalStatus.Lookup = 15 AND PeopleMaritalStatus.ID = Peoples_People.MaritalStatus /* Marital Status lookup */ 
LEFT JOIN Peoples_Employer ON Peoples_Employer.Company = Peoples_People.Company AND Peoples_Employer.ID = Peoples_People.Employer 
LEFT JOIN HomeTeam_LookUpValues AS DisabilityLookup ON DisabilityLookup.Company = Peoples_People.Company AND DisabilityLookup.Lookup = 564 AND DisabilityLookup.ID = Peoples_People.Disability 
LEFT JOIN HomeTeam_LookUpValues AS NonEnglish ON NonEnglish.Company = Peoples_People.Company AND NonEnglish.Lookup = 561 AND NonEnglish.ID = Peoples_People.NonEnglishSpeakingBackground 
inner join dbo.Companies_Company on Peoples_People.company = dbo.Companies_Company.id		
LEFT JOIN Peoples_People_TotalWeeklyIncome ON Peoples_People_TotalWeeklyIncome.Company = Peoples_People.Company AND Peoples_People_TotalWeeklyIncome.ID = Peoples_People.ID 
LEFT JOIN Peoples_People_TotalWeeklyWagesIncome ON Peoples_People_TotalWeeklyWagesIncome.Company = Peoples_People.Company 
AND Peoples_People_TotalWeeklyWagesIncome.ID = Peoples_People.ID 

INNER JOIN PropertyStructure_Unit ON Occupancies_Occupancy.Company = PropertyStructure_Unit.Company AND Occupancies_Occupancy.Unit = PropertyStructure_Unit.ID 
left join PropertyStructure_BlockUnit AS bb ON PropertyStructure_Unit.ID = bb.Unit 
LEFT OUTER JOIN PropertyStructure_Block AS cc ON bb.Block = cc.Code 
-- LEFT OUTER JOIN   PropertyStructure_District AS ee ON cc.District = ee.Code 
LEFT OUTER JOIN PropertyStructure_scheme AS dd ON cc.Scheme = dd.Code

WHERE (Peoples_People.Company = 1)  AND (1=0  OR Peoples_People.CurrentTenant = 1 )  and dd.code = 'BA'

ORDER BY Peoples_People.Surname ASC</property>
